@model MvcCV.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "BringSocialMedia";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Update Social Media Account</h4>
<br />
<form class="form-group" method="post">
    <br />
    @Html.Label("Account")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Icon")
    @Html.TextBoxFor(x => x.İkon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>
</form>

