@model MvcCV.Models.Entity.TblSertifikalarim

@{
    ViewBag.Title = "BringCertificate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Update Certificates</h4>
<br />
<form class="form-group" method="post">
    <br />
    @Html.Label("Certificates Description")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Certificates Date")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    @Html.Label("Certificates Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>
</form>
